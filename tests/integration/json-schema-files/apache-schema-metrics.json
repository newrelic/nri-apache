{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "",
  "properties": {
    "integration_version": {
      "minLength": 1,
      "pattern": "^1.[0-9]+.[0-9]+$",
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "events": {
            "items": {
              "properties": {},
              "required": []
            },
            "type": "array"
          },
          "inventory": {
            "maxProperties": 0,
            "properties": {},
            "required": [],
            "type": "object"
          },
          "metrics": {
            "items": [
              {
                "properties": {
                  "event_type": {
                    "minLength": 1,
                    "pattern": "^ApacheSample$",
                    "type": "string"
                  },
                  "net.bytesPerSecond": {
                    "type": "number"
                  },
                  "net.requestsPerSecond": {
                    "type": "number"
                  },
                  "server.busyWorkers": {
                    "type": "number"
                  },
                  "server.idleWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.closingWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.dnsLookupWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.finishingWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.idleCleanupWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.keepAliveWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.loggingWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.readingWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.startingWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.totalWorkers": {
                    "type": "number"
                  },
                  "server.scoreboard.writingWorkers": {
                    "type": "number"
                  },
                  "software.version": {
                    "minLength": 1,
                    "type": "string"
                  }
                },
                "required": [
                  "event_type",
                  "net.bytesPerSecond",
                  "net.requestsPerSecond",
                  "server.busyWorkers",
                  "server.idleWorkers",
                  "server.scoreboard.closingWorkers",
                  "server.scoreboard.dnsLookupWorkers",
                  "server.scoreboard.finishingWorkers",
                  "server.scoreboard.idleCleanupWorkers",
                  "server.scoreboard.keepAliveWorkers",
                  "server.scoreboard.loggingWorkers",
                  "server.scoreboard.readingWorkers",
                  "server.scoreboard.startingWorkers",
                  "server.scoreboard.totalWorkers",
                  "server.scoreboard.writingWorkers",
                  "software.version"
                ]
              }
            ],
            "minItems": 1,
            "type": "array",
            "uniqueItems": true
          }
        },
        "required": [
          "metrics",
          "inventory",
          "events"
        ]
      }
    },
    "name": {
      "minLength": 1,
      "pattern": "^com.newrelic.apache$",
      "type": "string"
    },
    "protocol_version": {
      "minLength": 1,
      "pattern": "^2$",
      "type": "string"
    }
  },
  "required": [
    "name",
    "protocol_version",
    "integration_version",
    "data"
  ],
  "type": "object"
}